/* pages/cart/cart.wxss */
.header {
  width: 100%;
  height: 80rpx;
  background-color: deepskyblue;
  text-align: center;
  line-height: 100rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}

.nav {
  display: flex;
  flex-wrap: wrap;
}

.nav>view {
  width: 31%;
  margin: 1%;
  text-align: center;
  background-color: black;
  border-radius: 20rpx;
}

.nav>view>button {
  font-size: 24rpx;
  color: deepskyblue;
  background-color: black;
}

.nav>view>text{
  font-size: 22rpx;
  color: skyblue;
  line-height: 22rpx;
}
