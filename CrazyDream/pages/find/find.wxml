<view style="padding:20rpx;">
  <canvas class="bagua logo" style="width:200px;border-radius:50%; height:200px;border:2rpx solid #000;position: absolute;z-index: 100; left: {{positionmsg.left}}px; top: {{positionmsg.top}}px;" canvas-id="firstCanvas"></canvas>
  <view style="background:#f5f5f5;height:60rpx;display:flex;">
    <button style="width:40rpx;height:60rpx;text-align:center">
      <image class="image1 " style="width:30rpx;height:30rpx;margin-left:-14rpx;" src="./image/sousuo.png" />
    </button>
    <input type="text" placeholder="输入你所梦到的内容" style="flex:1;text-indent:20rpx;height:100%" />
  </view>
  <view class="boss">
    <view class="box" wx:for="{{imgarr}}" wx:key="index">
      <image class="img1 logo1" src="{{item}}" />
      <text class="" selectable="false" space="false" decode="false">{{textarr[index]}}</text>
    </view>
  </view>
  <view class="text1" wx:for="{{textarr1}}" wx:key="index">{{item}}</view>
</view>